{
    "name": "googleapis-integration",
    "version": "0.0.1",
    "description": "Integration of googleapis",
    "repository": {
        "type": "git",
        "url": "https://github.com/EmitoSanti"
    },
    "author": "Emiliano Santi",
    "license": "MIT",
    "scripts": {
        "heroku-prebuild": "npm install",
        "start": "node dist/server.js",
        "dev": "npm run build && npm run watch && npm run serve",
        "build": "tsc",
        "serve": "node dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run watch-ts\" \"nodemon dist/server.js\"",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "heroku-postbuild": "npm run build"
    },
    "dependencies": {
        "nodemon": "^1.17.5",
        "body-parser": "^1.18.3",
        "compression": "^1.7.2",
        "cors": "^2.8.4",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "fs": "0.0.1-security",
        "googleapis": "^39.2.0",
        "helmet": "^3.12.1",
        "lodash": "^4.17.15",
        "mongodb": "^3.4.1",
        "mongoose": "^5.8.4",
        "morgan": "^1.9.1"
    },
    "devDependencies": {
        "@types/body-parser": "1.17.0",
        "@types/compression": "0.0.36",
        "@types/cors": "^2.8.4",
        "@types/dotenv": "^4.0.3",
        "@types/escape-html": "0.0.20",
        "@types/express": "^4.16.0",
        "@types/helmet": "0.0.37",
        "@types/lodash": "^4.14.149",
        "@types/mongodb": "^3.3.14",
        "@types/mongoose": "^5.5.38",
        "@types/morgan": "^1.7.35",
        "@types/node": "^8.9.4",
        "concurrently": "3.6.0",
        "ts-loader": "^3.5.0",
        "tslint": "^5.20.1",
        "typescript": "^3.8.3"
    },
    "engines": {
        "node": "12.16.1"
    }
}
